{
  "name": "webhook-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder -mw",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "axios": "^0.19.2",
    "highlight.js": "^10.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lottie": "^1.2.3",
    "simple-markdown": "^0.7.2",
    "source-map-support": "^0.5.16",
    "twemoji": "^12.1.6",
    "undux": "^5.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "electron": "13.6.6",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "webpack": "~4.42.1"
  },
  "electronWebpack": {
    "whiteListedModules": [
      "undux"
    ]
  },
  "build": {
    "appId": "com.shihab.webhook-app",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "src/static/icon.png"
    },
    "mac": {
      "icon": "src/static/icon.png"
    }
  }
}
